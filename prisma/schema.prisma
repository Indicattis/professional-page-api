generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Assessment {
  id           String @id @default(uuid())
  profile_name String
  profile_mail String
  profile_img  String
  rating_text  String
  rating_media Float
  loged_from   String

  createdAt DateTime @default(now())

  @@map("IND_Assessments")
}
